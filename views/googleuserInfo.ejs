<%- include('head'); -%>
<main>
  <div class="jumbotron">
      <p class="lead"><i class="fa fa-user" aria-hidden="true"></i>
        Google User Info</p>
      <hr class="my-4">
      <p>Sub: <%= user.sub  %></p>
      <p>Name: <%= user.name %></p>
      <p>Given Name: <%= user.given_name %></p>
      <p>Family Name: <%= user.family_name %></p>
      <img src="<%= user.picture %>" alt="Picture">
      <p>Email: <%= user.email %></p>
      <p>Email Verified: <%= user.email_verified %></p>
      <p>Locale: <%= user.locale %></p>
      <a class="btn btn-primary" href="/moreinfo/google" role="button"><span class="fa fa-user"></span>More Info</a>
  </div>
</main>
<%- include('foot'); -%>
